        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Activities.MainActivity" />
        <activity android:name=".Activities.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ClassicWorkout.ClassicInstructionsActivity" />
        <activity android:name=".ClassicWorkout.ListOfExerciseActivity" />
        <activity android:name=".ClassicWorkout.ClassicExerciseActivity" />
        <activity android:name=".AbsWorkout.ListOfAbsExercisesActivity" />
        <activity android:name=".AbsWorkout.AbsExerciseActivity" />
        <activity android:name=".AbsWorkout.ListOfAbsExercisesActivity" />
        <activity android:name=".AbsWorkout.AbsInstructionsActivity" />
        <activity android:name=".AbsWorkout.AbsExerciseActivity" />
        <activity android:name=".ArmWorkout.ArmExerciseActivity" />
        <activity android:name=".ArmWorkout.ArmInstructionsActivity" />
        <activity android:name=".ArmWorkout.ListOfArmExerciseActivity" />
        <activity android:name=".ButtWorkout.ButtExerciseActivity" />
        <activity android:name=".ButtWorkout.ButtInstructionsActivity" />
        <activity android:name=".ButtWorkout.ListOfButtExerciseActivity" />
        <activity android:name=".LegWorkout.LegExerciseActivity" />
        <activity android:name=".LegWorkout.LegInstructionsActivity" />
        <activity android:name=".LegWorkout.ListOfLegExerciseActivity" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />

        <activity android:name=".Activities.AllInstructionsActivity"></activity>
    </application>

</manifest>